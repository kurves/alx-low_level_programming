CC = gcc
SRC = school.c main.c
OBJ = $(SRC:.c=.o)
NAME = school
CFLAGS = -Wall -Werror -Wextra -pedantic
RM = rm -f

$(NAME): $(OBJ) m.h
	$(CC)  -Wall -Werror -Wextra -pedantic  $(OBJ) -o  $(NAME)


all: $(NAME)

clean:
	@$(RM) -f *~ *.swp $(NAME)

oclean:
	@$(RM) $(OBJ)

fclean: clean oclean

re: fclean all
